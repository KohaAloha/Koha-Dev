 [% ELSE %] <option value="c">haut (31x - 60x)</option>
 [% END %] [% IF ( f4d ) %] <option value="d" selected="selected">très haut (61x - 90x)</option>
 [% ELSE %] <option value="d">très haut (61x - 90x)</option>
 [% END %] [% IF ( f4e ) %] <option value="e" selected="selected">réduction ultraélevée (91x -)</option>
 [% ELSE %] <option value="e">réduction ultraélevée (91x -)</option>
 [% END %] [% IF ( f4u ) %] <option value="u" selected="selected">inconnu</option>
 [% ELSE %] <option value="u">inconnu</option>
 [% END %] [% IF ( f4v ) %] <option value="v" selected="selected">variable</option>
 [% ELSE %] <option value="v">variable</option>
 [% END %] </select>
 </td></tr>
 <tr>
 <td><label for="f5">Specific reduction ratio</label></td>
 <td><input type="text" name="f5" id="f5" maxlength="3" size="4" 
value="[% f5 %]" /></td>
 </tr>

 <tr>
 <td><label for="f6">Couleur&nbsp;:</label></td>
 <td>
