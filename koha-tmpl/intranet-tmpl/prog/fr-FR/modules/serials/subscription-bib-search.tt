<body id="ser_subscription-bib-search" class="ser">

<div id="doc" class="yui-t7">
 <div id="bd">

<h1>Recherche catalogue</h1>
[% IF ( no_query ) %] <div class="warning">Vous devez saisir un terme de recherche sur</div>
[% END %] <form name="f" action="/cgi-bin/koha/serials/subscription-bib-search.pl" method="get">
 <input type="hidden" name="op" value="do_search" />
 <input type="hidden" name="type" value="intranet" />
 <fieldset class="rows">
 <ol>
 <li><label for="q">Rechercher sur&nbsp;:</label>
 <input type="text" id="q"  name="q" /></li>
 
 <!-- ITEMTYPE LIMITS -->
 <li><label for="itemtypelimit">Limit type to: </label>
 <select name="itemtypelimit" id="itemtypelimit">
 <option value="">Tous</option>
 [% FOREACH itemtypeloo IN itemtypeloop %] <option value="[% itemtypeloo.code %]">
 [% itemtypeloo.description |html %] </option>
 [% END %] </select>
 </li>
 <!-- /ITEMTYPE LIMITS -->
 </ol>
</fieldset>
<fieldset class="action"><input value="Rechercher" type="submit" /></fieldset>


</form>

</div>

