 [% END %] [% IF ( f1t ) %] <option value="t" selected="selected">microforme</option>
 [% ELSE %] <option value="t">microforme</option>
 
 [% END %] [% IF ( f1z ) %] <option value="z" selected="selected">autre type de document</option>
 [% ELSE %] <option value="z">autre type de document</option>
 
 [% END %] </select>
 </td>
 </tr>
</table>
<fieldset class="action"><input type="button" value="OK"  onclick="report();" /> <a href="#" class="cancel close">Annuler</a></fieldset>
</form>
<script type="text/javascript">
//<![CDATA[
function report() {
        var doc   = opener.document; 
        var field = doc.getElementById("[% index %]");
    
        field.value = document.f_pop.f1.value;
        self.close();
        return false;
	}

//]]>
</script>


