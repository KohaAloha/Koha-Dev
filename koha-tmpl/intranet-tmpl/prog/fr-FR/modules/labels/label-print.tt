 [% INCLUDE 'doc-head-open.inc' %] <title>Koha &rsaquo; Outils &rsaquo; Etiquettes &rsaquo Impression/Export d'étiquettes</title>
 [% INCLUDE 'doc-head-close.inc' %] <script type="text/javascript">
        //<![CDATA[
            function Done() {
                window.location = "[% referer %]";
            };
        //]]>
    </script>
 <style type="text/css">#custom-doc {width:47.23em; *width:46.04em; min-width:610px; margin:auto; margin-top:0.4em;}</style>
 <style type="text/css">table {border-collapse: separate; border-spacing: 0; border: hidden none;} .header {cursor: auto; background-position: center center; background-repeat: repeat;}</style>
</head>
<body id="labels_label-print" class="tools labels">
 <div id="custom-doc" class="yui-t2">
 <div id="bd">
 [% IF ( batches ) %] <form>
 <h3>Cliquez sur le(s) lien(s) suivant(s) pour télécharger le(s) lot(s) exporté(s).</h3>
 [% FOREACH batche IN batches %] <fieldset>
 [% IF ( batche.label_ids ) %] <legend>[% IF ( batche.label_count == 1 ) %][% batche.label_count %] étiquette isolée[% ELSE %][% batche.label_count %] étiquettes isolées[% END %]</legend>
 <p><a class="document pdf" href="/cgi-bin/koha/labels/[% batche.create_script %]?batch_id=[% batche.batch_id %]&amp;template_id=[% batche.template_id %]&amp;layout_id=[% batche.layout_id %]&amp;start_label=[% batche.start_label %][% batche.label_ids %]">Télécharger PDF</a></p>

 <p><a class="document csv" href="/cgi-bin/koha/labels/label-create-csv.pl?batch_id=[% batche.batch_id %]&amp;template_id=[% batche.template_id %]&amp;layout_id=[% batche.layout_id %][% batche.label_ids %]">Télécharger CSV</a></p>

 <p><a class="document xml" href="/cgi-bin/koha/labels/label-create-xml.pl?batch_id=[% batche.batch_id %]&amp;template_id=[% batche.template_id %]&amp;layout_id=[% batche.layout_id %][% batche.label_ids %]">Télécharger XML</a></p>
 [% ELSIF ( batche.item_numbers ) %] <legend>[% IF ( batche.label_count == 1 ) %][% batche.label_count %] étiquette isolée[% ELSE %][% batche.label_count %] étiquettes isolées[% END %]</legend>
 <p><a class="document pdf" href="/cgi-bin/koha/labels/[% batche.create_script %]?template_id=[% batche.template_id %]&amp;layout_id=[% batche.layout_id %]&amp;start_label=[% batche.start_label %][% batche.item_numbers %]">Télécharger PDF</a></p>

 <p><a class="document csv" href="/cgi-bin/koha/labels/label-create-csv.pl?batch_id=[% batche.batch_id %]&amp;template_id=[% batche.template_id %]&amp;layout_id=[% batche.layout_id %][% batche.item_numbers %]">Télécharger CSV</a></p>

 <p><a class="document xml" href="/cgi-bin/koha/labels/label-create-xml.pl?batch_id=[% batche.batch_id %]&amp;template_id=[% batche.template_id %]&amp;layout_id=[% batche.layout_id %][% batche.item_numbers %]">Télécharger XML</a></p>
 [% ELSE %] <legend>Lots d'Étiquettes numéro [% batche.batch_id %]</legend>
 <p><a class="document pdf" href="/cgi-bin/koha/labels/label-create-pdf.pl?batch_id=[% batche.batch_id %]&amp;template_id=[% batche.template_id %]&amp;layout_id=[% batche.layout_id %]&amp;start_label=[% batche.start_label %]">Télécharger PDF</a>
 </p>
 <p>
 <a class="document csv" href="/cgi-bin/koha/labels/label-create-csv.pl?batch_id=[% batche.batch_id %]&amp;template_id=[% batche.template_id %]&amp;layout_id=[% batche.layout_id %]">Télécharger CSV</a>
 </p>
 <p>
 <a class="document xml" href="/cgi-bin/koha/labels/label-create-xml.pl?batch_id=[% batche.batch_id %]&amp;template_id=[% batche.template_id %]&amp;layout_id=[% batche.layout_id %]">Télécharger XML</a></p>
 [% END %] </fieldset>
 [% END %] <fieldset class="action">
 <input value="Fait" type="button" id="done" onclick="parent.parent.GB_hide();" class="submit" />
 </fieldset>
 </form>
 [% ELSE %] <h3>
 [% IF ( label_ids ) %] [% IF ( label_count == 1 ) %]Export [% label_count %] étiquette[% ELSE %]Export [% label_count %] étiquettes[% END %] [% ELSIF ( item_numbers ) %] [% IF ( item_count == 1 ) %]Export [% item_count %] étiquette[% ELSE %]Export [% item_count %] étiquettes[% END %] [% ELSE %] [% IF ( multi_batch_count == 1 ) %][% multi_batch_count %] lot à exporter[% ELSE %][% multi_batch_count %] lots à exporter[% END %] [% END %] </h3>
 <form name="exporting" method="post" action="/cgi-bin/koha/labels/label-print.pl">
 <input type="hidden" name="op" value="export" />
 <input type="hidden" name="referer" value="[% referer %]" />
 [% FOREACH batch_id IN batch_ids %] <input type="hidden" name="batch_id" value="[% batch_id.batch_id %]" />
 [% END %] [% FOREACH label_id IN label_ids %] <input type="hidden" name="label_id" value="[% label_id.label_id %]" />
 [% END %] [% FOREACH item_number IN item_numbers %] <input type="hidden" name="item_number" value="[% item_number.item_number %]" />
 [% END %] <fieldset class="rows">
 <ol>
 <li>
 <label for="template_id" style="width:20em">Sélectionnez un modèle à appliquer&nbsp;: </label>
 <select name="template_id" id="template_id">
 [% FOREACH template IN templates %] <option value="[% template.template_id %]">[% template.template_code %]</option>
 [% END %] </select>
 </li>
 <li>
 <label for="layout_id" style="width:20em">Sélectionnez un format à appliquer&nbsp;: </label>
 <select name="layout_id" id="layout_id">
 [% FOREACH layout IN layouts %] <option value="[% layout.layout_id %]">[% layout.layout_name %]</option>
 [% END %] </select>
 </li>
 <li>
 <label for="start_label" style="width:20em">Enter starting label position (for PDF): </label>
 <input value="1" name="start_label" type="text" id="start_label" class="focus" title="Commencer l'impression à l'étiquette numéro&nbsp;: " size="5" />
 </li>
 </ol>
 </fieldset>
 <fieldset class="action">
 <input value="Exportation" type="submit" class="submit" />
 <a href="#" class="cancel" id="done" onclick="parent.parent.GB_hide();return false;">Annuler</a>
 </fieldset>
 </form>
 [% END %] </div>
[% INCLUDE 'popup-bottom.inc' %] 