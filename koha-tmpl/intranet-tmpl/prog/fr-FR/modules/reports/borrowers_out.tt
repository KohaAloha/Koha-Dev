[% INCLUDE 'doc-head-open.inc' %] <title>Koha &rsaquo; Statistiques &rsaquo; Adhérents n'empruntant pas</title>
[% INCLUDE 'doc-head-close.inc' %] [% INCLUDE 'calendar.inc' %] <script type="text/javascript">
//<![CDATA[
$(document).ready(function(){
    $("#to").datepicker({ maxDate: "-1D" });
});
//]]>
</script>
</head>
<body id="rep_borrowers_out" class="rep">
[% INCLUDE 'header.inc' %] [% INCLUDE 'cat-search.inc' %] <div id="breadcrumbs"><a href="/cgi-bin/koha/mainpage.pl">Accueil</a> &rsaquo; <a href="/cgi-bin/koha/reports/reports-home.pl">Bilans et statistiques</a> &rsaquo; Adhérents n'ayant jamais emprunté</div>

<div id="doc3" class="yui-t2">
 
 <div id="bd">
 <div id="yui-main">
 <div class="yui-b">

[% IF ( do_it ) %] [% FOREACH mainloo IN mainloop %] <h1>Adhérents sans emprunts</h1>
 [% IF ( mainloo.loopfilter ) %] <p>Filtre sur&nbsp;:</p>
 [% FOREACH loopfilte IN mainloo.loopfilter %] <p>[% IF ( loopfilte.err ) %] [% END %] [% loopfilte.crit %] =[% loopfilte.filter %][% IF ( loopfilte.err ) %] [% END %]</p>
 [% END %] [% END %] <table>
 <tr>
 <th>N°/Adhérent</th>
 [% FOREACH loopco IN mainloo.loopcol %] <th>[% loopco.coltitle %]</th>
 [% END %] </tr>
 [% FOREACH loopro IN mainloo.looprow %] [% UNLESS ( loop.odd ) %] <tr class="highlight">
 [% ELSE %] <tr>
 [% END %] <td>[% loopro.rowtitle %]</td>
 [% FOREACH loopcel IN loopro.loopcell %] <td>[% IF ( loopcel.value ) %][% loopcel.value %][% END %] </td>
 [% END %] </tr>
 [% END %] </table>
 [% END %] [% ELSE %] <h1>Adhérents sans emprunts</h1>
 <form method="post" action="/cgi-bin/koha/reports/borrowers_out.pl">
 <fieldset class="rows">
 <ol>
 <li><label for="patroncategory">Catégorie adhérent&nbsp;: </label> <select name="Filter" id="patroncategory"><option value="" > Tout code catégorie</option>
 [% FOREACH borcatloo IN borcatloop %] <option value="[% borcatloo.value %]" >[% borcatloo.description %] </option> 
 [% END %] </select>
</li>
 <li><label for="to">Pas prété depuis&nbsp;:</label> <input size="10" id="to" name="Filter" value="" type="text" />
</li>
 </ol>
 </fieldset>
 
 <fieldset class="rows">
 <legend>Limits</legend>
 <ol>
 <li><label for="numberlimit">Limit to: </label><select name="Limit" id="numberlimit">
 <option value ="" selected="selected">Aucun</option>
 <option value ="5"> 5</option>
 <option value ="10">10</option>
 <option value ="15">15</option>
 <option value ="20">20</option>
 <option value ="25">25</option>
 <option value ="40">40</option>
 <option value ="50">50</option>
 <option value ="100">100</option>
 </select></li>
 <li><label for="criteria">Par&nbsp;: </label><select name="Criteria" id="criteria">
 <option value ="" selected="selected">Aucun</option>
 <option value ="categorycode">Catégorie adhérent</option>
 <option value ="branchcode">Library</option>
 </select></li>
 </ol>
 </fieldset>
 
<fieldset class="rows">
 <legend>Affichage</legend>
<ol><li><label for="outputscreen">Dans le navigateur</label><input type="radio" checked="checked" name="output" id="outputscreen" value="screen" /> </li>
<li><label for="outputfile">Vers un fichier&nbsp;:</label> <input type="radio" name="output" value="file" id="outputfile" /> <label class="inline" for="basename">Named: </label><input type="text" name="basename" id="basename" value="Export" /> <label class="inline" for="MIME">Dans </label>[% CGIextChoice %] [% CGIsepChoice %]</li></ol>
 </fieldset>

 <fieldset class="action">
 <input value="Valider" type="submit" />
 <input type="hidden" name="report_name" value="[% report_name %]" />
 <input type="hidden" name="do_it" value="1" />
 </fieldset>
 </form>
[% END %] </div>
</div>
<div class="yui-b">
[% INCLUDE 'reports-menu.inc' %] </div>
</div>
[% INCLUDE 'intranet-bottom.inc' %] 