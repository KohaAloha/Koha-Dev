[% INCLUDE 'doc-head-open.inc' %] <title>Koha &rsaquo; Acquisitions &rsaquo; Réceptionner colis du fournisseur [% name %]</title>
<link rel="stylesheet" type="text/css" href="[% themelang %]/css/datatables.css" />
[% INCLUDE 'doc-head-close.inc' %] [% INCLUDE 'calendar.inc' %] <script type="text/javascript" src="[% themelang %]/lib/jquery/plugins/jquery.dataTables.min.js"></script>
[% INCLUDE 'datatables-strings.inc' %] <script type="text/javascript" src="[% themelang %]/js/datatables.js"></script>
<script type="text/javascript">
//<![CDATA[
 $(document).ready(function() {
[% IF (dateformat == 'metric') %]
    dt_add_type_uk_date();
[% END %]
    var parcelst = $("#parcelst").dataTable($.extend(true, {}, dataTablesDefaults, {
        "sPaginationType": "four_button"
    } ) );
 });
 //]]>
</script>
</head>
<body id="acq_parcels" class="acq">
[% INCLUDE 'header.inc' %] [% INCLUDE 'acquisitions-search.inc' %] <div id="breadcrumbs"><a href="/cgi-bin/koha/mainpage.pl">Accueil</a> &rsaquo; <a href="/cgi-bin/koha/acqui/acqui-home.pl">Acquisitions</a> &rsaquo; <a href="/cgi-bin/koha/acqui/supplier.pl?booksellerid=[% booksellerid %]">[% name %]</a> &rsaquo; Récevoir le colis du fournisseur [% name %]</div>

[% IF ( count ) %]<div id="doc3" class="yui-t2">[% ELSE %]<div id="doc" class="yui-t7">[% END %] <div id="bd">
 <div id="yui-main">
 <div class="yui-b">

[% IF ( error_failed_to_create_invoice ) %] <div id="error" class="dialog error">
 <p>An error has occured. Invoice cannot be created.</p>
 </div>
[% END %] <h1>Réception de colis du fournisseur <a href="/cgi-bin/koha/acqui/supplier.pl?booksellerid=[% booksellerid %]">[% name %]</a></h1>

[% IF ( count ) %] <p> [% count %] colis</p>
<div id="resultlist">
<!-- Search Results Table -->

<table class="small" id="parcelst">
 <thead>
 <tr>
 <th>Line</th>
 <th>Date received</th>
 <th>Récérence fournisseur</th>
 <th>Décompte des documents</th>
 <th>Nombre de notices</th>
 <th>Document(s) attendu(s)</th>
 </tr>
 </thead>
 <tbody>
 <!-- Actual Search Results -->
 [% FOREACH searchresult IN searchresults %]
 [% UNLESS ( loop.odd ) %]
 <tr class="highlight">
 [% ELSE %] <tr>
 [% END %] <td>
 [% searchresult.number %] </td>
 <td>
 [% searchresult.datereceived %] </td>
 <td>
 [% IF ( searchresult.code ) %] <a href="/cgi-bin/koha/acqui/parcel.pl?invoiceid=[% searchresult.invoiceid %]">[% searchresult.code %]</a>
 [% ELSE %] <acronym title="Indisponible">n/a</acronym>
 [% END %] </td>
 <td>
 [% searchresult.reccount %] </td>
 <td>
 [% searchresult.bibcount %] </td>
 <td>
 [% searchresult.itemcount %] </td>
 </tr>
 [% END %] </tbody>
</table>

<div id="resultnumber">
<!-- Row of numbers corresponding to search result pages -->
[% IF ( displayprev ) %]
 <a href="parcels.pl?booksellerid=[% booksellerid %]&amp;startfrom=[% prevstartfrom %][% IF ( datefrom ) %]&amp;datefrom=[% datefrom %][% END %][% IF ( dateto ) %]&amp;dateto=[% dateto %][% END %][% IF ( code ) %]&amp;filter=[% code %][% END %][% IF ( orderby ) %]&amp;orderby=[% orderby %][% END %][% IF ( resultsperpage ) %]&amp;resultsperpage=[% resultsperpage %][% END %]&amp;type=intra">&lt;&lt; Précédent</a>
[% END %] [% FOREACH number IN numbers %] [% IF ( number.highlight ) %] <span class="current">[% number.number %]</span>
 [% ELSE %] <a href="parcels.pl?booksellerid=[% number.booksellerid %]&amp;startfrom=[% number.startfrom %][% IF ( number.datefrom ) %]&amp;datefrom=[% number.datefrom %][% END %][% IF ( number.dateto ) %]&amp;dateto=[% number.dateto %][% END %][% IF ( number.code ) %]&amp;filter=[% number.code %][% END %][% IF ( number.orderby ) %]&amp;orderby=[% number.orderby %][% END %][% IF ( number.resultsperpage ) %]&amp;resultsperpage=[% number.resultsperpage %][% END %]&amp;type=intra">[% number.number %]</a>
 [% END %] [% END %] [% IF ( displaynext ) %] <a href="parcels.pl?booksellerid=[% booksellerid %]&amp;startfrom=[% nextstartfrom %][% IF ( datefrom ) %]&amp;datefrom=[% datefrom %][% END %][% IF ( dateto ) %]&amp;dateto=[% dateto %][% END %][% IF ( code ) %]&amp;filter=[% code %][% END %][% IF ( orderby ) %]&amp;orderby=[% orderby %][% END %][% IF ( resultsperpage ) %]&amp;resultsperpage=[% resultsperpage %][% END %]&amp;type=intra">Suivant >></a>
[% END %] </div>
</div>
[% END %] <div id="parcels_new_parcel">
 <form method="get" action="parcels.pl">
 <fieldset class="rows">
 <legend>Réceptionner un nouveau colis</legend>
 <ol> <li>
 <label for="invoice">Facture fournisseur </label>
 <input type="hidden" name="booksellerid" value="[% booksellerid %]" />
 <input type="hidden" name="op" value="new" />
 <input type="text" size="20" id="invoice" name="invoice" />
 </li>
 [% IF ( gst ) %] <li>
 <label for="gst">TVA&nbsp;:</label>
 <input type="text" size="20" id="gst" name="gst" />
 </li>
 [% END %]
 <!-- // Removing freight input until shipping can be proplerly handled .
 <li>
 <label for="freight">Shipping:</label>
 <input type="text" size="20" id="freight" name="freight" />
 </li> -->
 <li>
 <label for="shipmentdate">Date d'expédition&nbsp;: </label>
 <input type="text" id="shipmentdate" name="shipmentdate" maxlength="10" size="10" value="[% shipmentdate_today %]" class="datepicker" />
 <div class="hint">[% INCLUDE 'date-format.inc' %]</div>
 </li>
 <li>
 <label for="shipmentcost">Shipment cost: </label>
 <input type="text" id="shipmentcost" name="shipmentcost" size="10" />
 </li>
 <li>
 <label for="shipmentcost_budgetid">Poste budgétaire&nbsp;: </label>
 <select id="shipmentcost_budgetid" name="shipmentcost_budgetid">
 <option value="">No fund</option>
 [% FOREACH budget IN budgets %] <option value="[% budget.budget_id %]">[% budget.budget_name %]</option>
 [% END %] </select>
 </li>
 </ol>
 </fieldset>
 <fieldset class="action"><input value="Suivant" type="submit" class="button" /> <a class="cancel" href="/cgi-bin/koha/acqui/supplier.pl?booksellerid=[% booksellerid %]">Annuler</a></fieldset>
 </form>
 </div>
</div>
</div>
<div class="yui-b">
[% IF ( count ) %]<form method="get" action="parcels.pl">
 <fieldset class="brief">
 <h4>Filtrer</h4>
 <ol>
 <li> <input type="hidden" name="booksellerid" value="[% booksellerid %]" /></li>
 <li><label for="filter">Référence fournisseur</label><input type="text" size="20" name="filter" value="[% filter %]" id="filter" /></li>
 <li><label for="datefrom">Du&nbsp;:</label><input type="text" size="9" id="datefrom" name="datefrom" value="[% datefrom %]" /><br /> 
 <label for="dateto">et le&nbsp;:</label><input type="text" size="9" id="dateto" name="dateto" value="[% dateto %]" /></li>
 <li><label for="orderby">Sort by :</label><select name="orderby" id="orderby">
 <option value="invoicenumber">Récérence fournisseur</option>
 <option value="shipmentdate">Shipment date</option>
 <option value="shipmentdate desc">Shipment date reverse</option>
 <option value="invoicenumber desc">Référence fournisseur inverse</option>
 </select><br />
 <label for="resultsperpage">Réponses par page&nbsp;: </label><select name="resultsperpage" id="resultsperpage">
 <option value="20">20</option>
 <option value="30">30</option>
 <option value="50">50</option>
 <option value="100">100</option>
 </select></li>
 </ol>
 <fieldset class="action"><input value="Filtrer" type="submit" class="button" /> <a href="/cgi-bin/koha/acqui/parcels.pl?booksellerid=[% booksellerid %]">Effacer</a></fieldset>
 </fieldset>
</form>[% END %] </div>
</div>
[% INCLUDE 'intranet-bottom.inc' %] 