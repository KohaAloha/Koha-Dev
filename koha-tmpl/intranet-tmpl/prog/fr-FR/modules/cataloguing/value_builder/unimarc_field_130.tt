 [% ELSE %] <option value="a">aperture card</option>
 [% END %] [% IF ( f1b ) %] <option value="b" selected="selected">cartouche microforme</option>
 [% ELSE %] <option value="b">cartouche microforme</option>
 [% END %] [% IF ( f1c ) %] <option value="c" selected="selected">cassette microfiche</option>
 [% ELSE %] <option value="c">cassette microfiche</option>
 [% END %] [% IF ( f1d ) %] <option value="d" selected="selected">microfilm</option>
 [% ELSE %] <option value="d">microfilm</option>
 [% END %] [% IF ( f1e ) %] <option value="e" selected="selected">microfiche</option>
 [% ELSE %] <option value="e">microfiche</option>
 [% END %] [% IF ( f1f ) %] <option value="f" selected="selected">cassette microfiche</option>
 [% ELSE %] <option value="f">cassette microfiche</option>
 [% END %] [% IF ( f1g ) %] <option value="g" selected="selected">micro opaque</option>
 [% ELSE %] <option value="g">micro opaque</option>
 [% END %] [% IF ( f1h ) %] <option value="h" selected="selected">microfilm</option>
 [% ELSE %] <option value="h">microfilm</option>
 [% END %] [% IF ( f1z ) %] <option value="z" selected="selected">autre</option>
 [% ELSE %] <option value="z">autre</option>
 [% END %] </select>
 </td></tr>
 <tr>
 <td><label for="f2">Polarité&nbsp;:</label></td>
 <td>
 <select name="f2" id="f2" size="1">

 [% IF ( f2a ) %] <option value="a" selected="selected">positive</option>
 [% ELSE %] <option value="a">positive</option>
 [% END %] [% IF ( f2b ) %] <option value="b" selected="selected">negatif</option>
 [% ELSE %] <option value="b">negatif</option>
 [% END %] [% IF ( f2c ) %] <option value="c" selected="selected">inconnu</option>
 [% ELSE %] <option value="c">inconnu</option>
 [% END %] [% IF ( f2d ) %] <option value="d" selected="selected">polarités diverses</option>
